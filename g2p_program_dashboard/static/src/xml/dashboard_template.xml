<?xml version="1.0" encoding="UTF-8" ?>
<template id="template" xml:space="preserve">
    <t t-name="ProgramDashBoard">
        <div class="programs-dashboard-wrap">
            <div
                class="o_dashboards col-xs-12 col-sm-12 col-lg-12 col-md-12"
                style="background-color: #e1e1e1;overflow: scroll; !important; "
            >
                <div class="content-header">
                    <div class="container-fluid">
                        <div class="row mb-2">
                            <div class="col-sm-12">
                                <div class="dash-header">
                                    <h1 class="custom-h1 dashboard-h1">Dashboard </h1>
                                    <!-- <input type="checkbox" style="display:none" data-toggle="toggle" data-on="" data-off="">
                                        <input type="checkbox" id="toggle-two"/>
                                    </input> -->
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row" style="margin:0px">
                <div class="col-xs-12 col-sm-12 col-lg-12 col-md-12">
                    <div class="">
                        <div class="row program-details" style="margin:0px">
                            <div class="col-md-3">
                                <!--Total Number of Programs-->
                                <div class="tile wide program box-1">
                                    <div class="headers">
                                        <div class="main-title">Number of Programs</div>
                                        <div id="program_statistics">
                                            <div class="left">
                                                <div class="count">
                                                    <span id="total_number_active_programs" />
                                                </div>
                                            </div>
                                            <div class="right">
                                                <div class="count">
                                                    <span id="total_number_ended_programs" />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <!--Total Number of Cycles-->
                                <div class="tile wide program box-2">
                                    <div class="headers">
                                        <div class="main-title">Number of Cycles</div>
                                        <div id="program_statistics">
                                            <div class="left">
                                                <div class="count">
                                                    <span id="total_number_new_cycles" />
                                                </div>
                                            </div>
                                            <div class="right">
                                                <div class="count">
                                                    <span id="total_number_approved_cycles" />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <!--Total Number of Vouchers-->
                                <div class="tile wide program box-3">
                                    <div class="headers">
                                        <div class="main-title">Number of Vouchers</div>
                                        <div id="program_statistics">
                                            <div class="left">
                                                <div class="count">
                                                    <span id="total_number_new_vouchers" />
                                                </div>
                                            </div>
                                            <div class="right">
                                                <div class="count">
                                                    <span id="total_number_approved_vouchers" />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <!-- Beneficiaries -->
                                <div class="tile wide program box-4">
                                    <div class="headers">
                                        <div class="main-title">Beneficiaries</div>
                                        <div id="program_statistics">
                                            <div class="left">
                                                <div class="count">
                                                    <!-- <span id="total_number_new_vouchers"/> -->
                                                </div>
                                            </div>
                                            <div class="right">
                                                <div class="count">
                                                    <!-- <span id="total_number_approved_vouchers"/> -->
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-xs-12 col-sm-12 col-lg-12 col-md-12">
                <div class="row" style="margin:0px">
                    <div class="col-md-4" id="col-graph">
                        <div class="card">
                            <div class="card-header">
                                <div class="card-title">
                                    <b>
                                        <h3 class="custom-h3">Programs</h3>
                                    </b>
                                </div>
                                <div class="card-tools">
                                    <select id="program_values" class="btn btn-primary">
                                        <option
                                            id="programs_this_year"
                                            value="programs_this_year"
                                        >This Year</option>
                                        <option
                                            id="programs_this_month"
                                            value="programs_this_month"
                                            selected=""
                                        >
                                            This Month</option>
                                        <div role="separator" class="dropdown-divider" />
                                        <option
                                            id="programs_last_month"
                                            value="programs_last_month"
                                        >Last Month</option>
                                        <option
                                            id="programs_last_year"
                                            value="programs_last_year"
                                        >Last Year</option>
                                    </select>
                                </div>
                            </div>
                            <div class="card-body mt-3" id="in_ex_body_hide">
                                <div class="row">
                                    <div class="col-md-12">
                                        <p id="myelement1"> </p>
                                        <div class="chart">
                                            <canvas id="canvas" width="300" height="200" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4" id="col-graph">
                        <div class="card">
                            <div class="card-header">
                                <div class="card-title">
                                    <b>
                                        <h3 class="custom-h3">VOUCHERS and PROGRAM FUNDS</h3>
                                    </b>
                                </div>
                                <div class="card-tools">
                                    <select id="voucher_values" class="btn btn-primary">
                                        <option id="voucher_this_month" value="this_month">This Month</option>
                                        <option id="voucher_this_year" value="this_year">This Year</option>
                                    </select>
                                </div>
                            </div>
                            <!--card-header-->
                            <div class="card-body" id="">
                                <div class="row">
                                    <div class="col-md-12 mt-3">
                                        <h1 class="custom-h1" style="margin-bottom: 0;">Vouchers</h1>
                                        <ul class="item-list">
                                            <li
                                                class="item"
                                                style="display: flex;justify-content: space-between;color: #000;"
                                            >
                                                <p id="total_voucher_paid" />
                                                <p id="total_voucher" />
                                                <p id="total_voucher_paid_current_year" />
                                                <p id="total_voucher_current_year" />
                                                <p id="total_voucher_paid_current_month" />
                                                <p id="total_voucher_current_month" />
                                            </li>
                                            <li>
                                                <progress id="tot_voucher" class="item-1" max="" value="">
                                                    <strong>Level: 50%</strong>
                                                </progress>
                                            </li>
                                            <li>
                                                <progress
                                                    id="tot_voucher_current_year"
                                                    class="item-1"
                                                    max=""
                                                    value=""
                                                >
                                                    <strong>Level: 50%</strong>
                                                </progress>
                                            </li>
                                            <li>
                                                <progress
                                                    id="tot_voucher_current_month"
                                                    class="item-1"
                                                    max=""
                                                    value=""
                                                >
                                                    <strong>Level: 50%</strong>
                                                </progress>
                                            </li>
                                        </ul>
                                        <div role="separator" class="dropdown-divider" />
                                        <h1 class="custom-h1" style="margin-bottom: 0;">Program Funds</h1>
                                        <ul class="item-list">
                                            <li
                                                class="item"
                                                style="display: flex;justify-content: space-between;color: #000;"
                                            >
                                                <p id="total_funds_received" />
                                                <p id="total_funds" />
                                                <p id="total_funds_received_current_year" />
                                                <p id="total_funds_current_year" />
                                                <p id="total_funds_received_current_month" />
                                                <p id="total_funds_current_month" />
                                            </li>
                                            <li>
                                                <progress id="tot_funds_rcv" class="item-1" max="" value="">
                                                    <strong>Level: 50%</strong>
                                                </progress>
                                            </li>
                                            <li>
                                                <progress
                                                    id="tot_funds_rcv_current_year"
                                                    class="item-1"
                                                    max=""
                                                    value=""
                                                >
                                                    <strong>Level: 50%</strong>
                                                </progress>
                                            </li>
                                            <li>
                                                <progress
                                                    id="tot_funds_rcv_current_month"
                                                    class="item-1"
                                                    max=""
                                                    value=""
                                                >
                                                    <strong>Level: 50%</strong>
                                                </progress>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>

            <div class="container-fluid o_in_dashboard">
                <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3" id="program_grapg" />
                <div class="dashboard-header-filter">
                    <div class="manager_filter_class" />
                </div>
            </div>
            <div id="chart-container" />
        </div>
    </t>
</template>
