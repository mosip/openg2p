<odoo>
    <data noupdate="1">
        <record id="paperformat_voucher_card" model="report.paperformat">
            <field name="name">Voucher Card</field>
            <field name="default" eval="False" />
            <field name="format">custom</field>
            <field name="page_height">90</field>
            <field name="page_width">222</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">4</field>
            <field name="margin_bottom">4</field>
            <field name="margin_left">2</field>
            <field name="margin_right">2</field>
            <field name="header_line" eval="False" />
            <field name="header_spacing">3</field>
            <field name="dpi">90</field>
        </record>


        <record id="action_voucher" model="ir.actions.report">
            <field name="name">Print Ticket</field>
            <field name="model">g2p.voucher</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">g2p_programs.print_voucher_card</field>
            <field name="report_file">g2p_programs.print_voucher_card</field>
            <field name="print_report_name">'Voucher Card'</field>
            <field name="binding_model_id" ref="model_g2p_voucher" />
            <field name="paperformat_id" ref="g2p_programs.paperformat_voucher_card" />
            <field name="binding_type">report</field>
        </record>



    </data>
</odoo>
