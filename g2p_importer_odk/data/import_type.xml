    </field>
  </record>

  <!-- res.partner group -->
  <record id="import_type_res_partner_group_data" model="import.type">
    <field name="name">Import ResPartner Group Data</field>
    <field name="key">res_partner_group_data</field>
    <field name="options">
- model: res.partner
  importer: g2p.res.partner.group.data.importer
    </field>
  </record>

</odoo>
